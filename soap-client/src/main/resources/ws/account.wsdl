      <xs:enumeration value="BalanceWithdrawal"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="AccountMovementList">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" name="accountMovement" type="tns:AccountMovement"/>
